      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${ICApproval=='Approved'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent4" name="End"></endEvent>
    <sequenceFlow id="flow38" name="IC Rejected OR Short Circuit" sourceRef="exclusivegateway7" targetRef="endevent4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${ICApproval=='Rejected' || shortCircuit=='Y'}]]></conditionExpression>
    </sequenceFlow>
